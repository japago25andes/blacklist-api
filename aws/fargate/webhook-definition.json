{
  "webhook": {
    "name": "blacklist-webhook",
    "targetPipeline": "pipeline-blacklist-api",
    "targetAction": "GitHub_Source",
    "filters": [
      {
        "jsonPath": "$.ref",
        "matchEquals": "refs/heads/main"
      }
    ],
    "authentication": "GITHUB_HMAC",
    "authenticationConfiguration": {
      "SecretToken": "TU_SECRET_TOKEN"
    }
  }
}